package koşullarörnek2;
public class Koşullarörnek2 {
    public static void main(String[] args) {
        for (int i = 1 ; i < 10 ; i++){
                    System.out.println("*****************");
            for (int k = 1 ; k < 10 ; k++){
                System.out.println(i + "x" + k + "=" + (i*k));         
            }
        }
        System.out.println("******************");
    }
}
