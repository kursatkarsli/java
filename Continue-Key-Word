package pkgcontinue.pkg2;
public class Continue2 {
    public static void main(String[] args) {
        int  i=0;          
        while (i < 10) {
        if (i== 3 || i == 5){
        continue;
        }
            System.out.println("i = " + i);      
            i++ ;
        }
    }
}
